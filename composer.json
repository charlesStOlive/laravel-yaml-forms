{
    "name": "waka/yamlforms",
    "description": "This is my package yamlforms",
    "keywords": [
        "Waka",
        "laravel",
        "yamlforms"
    ],
    "homepage": "https://github.com/waka/yamlforms",
    "license": "MIT",
    "authors": [
        {
            "name": "Charles",
            "email": "charles@notilac.fr",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "rcrowe/twigbridge": "^0.14.1",
        "spatie/laravel-package-tools": "^1.14.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^6.1",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.6",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Waka\\YamlForms\\": "src",
            "Waka\\YamlForms\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Waka\\YamlForms\\Tests\\": "tests"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Waka\\YamlForms\\YamlFormsServiceProvider"
            ],
            "aliases": {
                "YamlForms": "Waka\\YamlForms\\Facades\\YamlForms"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
